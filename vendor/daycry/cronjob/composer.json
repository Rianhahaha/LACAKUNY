{
    "name": "daycry/cronjob",
    "description": "Cronjob library for Codeigniter 4",
    "homepage": "https://github.com/daycry/cronjob",
    "authors":
    [
        {
            "name": "daycry",
            "homepage": "https://github.com/daycry",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "dragonmantank/cron-expression": "^3.3"
    },
    "require-dev":
    {
        "phpunit/phpunit" : "^9.5",
        "codeigniter4/framework": "^4",
        "friendsofphp/php-cs-fixer": "3.6.*",
        "mikey179/vfsstream": "^1.6",
        "nexusphp/cs-config": "^3.3",
        "nexusphp/tachycardia": "^1.0",
        "phpstan/phpstan": "^1.7.1",
        "rector/rector": "0.13.9"
    },
    "autoload":
    {
        "psr-4": {
            "Daycry\\CronJob\\": "src/"
        }
    },
    "autoload-dev":
    {
        "psr-4":
        {
            "Tests\\Support\\": "tests/_support"
        }
    },
    "scripts": {
        "test": "phpunit",
        "cs-fixer": "vendor/bin/php-cs-fixer fix src",
        "cs-fixer-tests": "vendor/bin/php-cs-fixer fix tests"
    }
}
